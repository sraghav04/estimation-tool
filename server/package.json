{
  "name": "estimation-tool",
  "version": "1.0.0",
  "description": "RESTful API with Node.js, Express, and PostgreSQL",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "ci": "yarn lint && yarn test",
    "coverage:ci": "jest --ci --reporters=jest-junit --coverage --coverageReporters=cobertura",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\" \"src/swagger.json\"",
    "format:check": "prettier --check \"{src,test}/**/*.js\" \"src/swagger.json\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node .",
    "start:dev": "npx nodemon .",
    "test:ci": "jest --ci --reporters=jest-junit --reporters=default",
    "test": "jest --config ./jest-config.js --verbose",
    "test:coverage": "jest --coverage --config ./jest-config.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jest-junit": "^14.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mammoth": "^1.9.0",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.9.5",
    "openai": "^4.96.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.7.3",
    "sequelize": "^6.21.3",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^4.5.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "eslint": "^8.21.0",
    "eslint-plugin-jest": "^26.8.2",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.4.1"
  }
}
